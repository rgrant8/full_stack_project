<!DOCTYPE html>
<%= csrf_meta_tags %>
<div class="fill"></div>
<head>
  <title>Market View</title>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true, :id => 'pagestyle' %>
  <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
  <script src="http://code.highcharts.com/stock/highstock.js"></script>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>
<body id="preload">
<div class="main">
 <header id="header">
  <div id="color_header">
   <div class="container">
    <div class="header-holder">           
     <div id='cssmenu'>
      <ul>     
        <li class="<%= current_page(root_path)%>"><%= link_to "Home", root_path, :id => "list-btn",
              :class => "loader move_to list", :method => :get %></li>
        <li class="<%= current_page(stock_user_lists_path)%>"><%= link_to "List", stock_user_lists_path, :id => "list-btn",:class => "loader move_to stock_user_lists_path", :method => :get %></li>
        <li class="<%= current_page(lists_path)%>"><%= link_to "Nasdaq", lists_path, :id => "list-btn1",
              :class => "loader move_to list", :method => :get %></li>
        <li class="<%= current_page(nyses_path)%>"><%= link_to "Nyse", nyses_path, 'data-no-turbolink' => true, :id => "list-btn2",
              :class => "loader move_to nyse", :method => :get %></li>
        <li class="<%= current_page(amexes_path)%>"><%= link_to "Amex", amexes_path, :id => "list-btn3",
              :class => "loader move_to amex", :method => :get %></li>
        <!-- Render correct login; depends on status -->
        <div id="log" style="float:right;"> <%= render 'devise/menu/login_items' %> </div> 
        <div id="reg" style="float:right;"> <%= render 'devise/menu/registration_items' %></div>
        <div id="mes" style="float:right; padding: 19px; font-size:10px"> <%= render 'devise/menu/message_items' %></div>
      </ul>
     </div>
    </div><!-- navbar-collapse -->
   </div><!-- container-fluid -->
  </div>
 </header>
<div id="contents"><!-- Ajax wrapper for -->
  <div id="yield"> 
    <%= yield %>
  </div>
</div>

</div>
<div id="footer">
  <div id="foot"> 
    Copyright 2016 RayLeon
  </div>
</div> 
<script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-75062649-1', 'auto');
  ga('send', 'pageview');
</script>
</body>
  <div class="modal"></div>
</html>


